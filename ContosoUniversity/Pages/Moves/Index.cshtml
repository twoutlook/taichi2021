@page
@model ContosoUniversity.Pages.Moves.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
     
            <th>
               combo name
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Move[0].Seq)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Move[0].Text)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Move) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Combo.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seq)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Text)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
